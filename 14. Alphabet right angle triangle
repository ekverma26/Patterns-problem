public class Main {
  public static void main(String[] args) {
    
    int i,j;
    int row=5;
    int n=1;
    
    for(i=1;i<=row;i++)
    {
   
    for(j=1;j<=i;j++)
    {
     System.out.print((char)(j+64));
     System.out.print(" ");
    }
    System.out.println();
    }
    
    
  }
}


OUTPUT:
A 
A B 
A B C 
A B C D 
A B C D E 
